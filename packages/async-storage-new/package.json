{
  "name": "@react-native-async-storage/async-storage-new",
  "version": "3.0.0",
  "description": "Asynchronous, persistent, key-value storage system for React Native.",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    }
  },
  "scripts": {
    "prepare": "bob build",
    "test:ts": "tsc --noEmit",
    "test:lint": "eslint src/*.{js,ts,tsx}",
    "clean": "del-cli android/build lib"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "devDependencies": {
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@types/react": "^19.1.0",
    "commitlint": "^19.6.1",
    "del-cli": "^5.1.0",
    "eslint": "9.34.0",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-builder-bob": "^0.40.13",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "AsyncStorageSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.asyncstorage"
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "version": "0.53.0"
  },
  "keywords": [
    "react-native",
    "react native",
    "async storage",
    "asyncstorage",
    "storage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-native-async-storage/async-storage.git",
    "directory": "packages/default-storage-backend"
  },
  "author": "Krzysztof Borowy <contact@kborowy.com> (https://www.kborowy.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-native-async-storage/async-storage/issues"
  },
  "homepage": "https://github.com/react-native-async-storage/async-storage#readme"
}
